<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	
	
	<b:Container b:id="checkin-container">
		<b:Form>
			<b:FieldSet>
		    <b:Legend>Check In form</b:Legend>
		
		 	<b:FormGroup ui:field="formGroupName">
		      <b:FormLabel ui:field="labelName" for="formName">Name</b:FormLabel>
		      <b:TextBox ui:field="userName" placeholder="blabla" b:id="formName"/>
		    </b:FormGroup>
		   	    
		    <b:FormGroup ui:field="formGroupPass1">
		      <b:FormLabel ui:field="labelPass1" for="formPassword">Password</b:FormLabel>
		      <b:Input ui:field="userPass1" type="PASSWORD" b:id="formPassword"/>
		    </b:FormGroup>
		    
		    <b:FormGroup ui:field="formGroupPass2">
		      <b:FormLabel ui:field="labelPass2" for="formPassword2">Password</b:FormLabel>
		      <b:Input ui:field="userPass2" type="PASSWORD" b:id="formPassword2"/>
		    </b:FormGroup>
		
		    <b:Button ui:field="btnConfirm" type="SUCCESS">Accept</b:Button>
		    <b:Label ui:field="stateMsg"></b:Label>
		  </b:FieldSet>
		</b:Form>
	</b:Container>
</ui:UiBinder> 